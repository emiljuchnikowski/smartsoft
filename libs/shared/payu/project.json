{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/payu/src",
  "projectType": "library",
  "generators": {},
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/shared/payu/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/shared/payu/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": [
        "{workspaceRoot}/coverage/libs/shared/payu"
      ]
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "outputPath": "dist/libs/shared/payu",
        "tsConfig": "libs/shared/payu/tsconfig.lib.json",
        "packageJson": "libs/shared/payu/package.json",
        "main": "libs/shared/payu/src/index.ts",
        "assets": [
          "libs/shared/payu/*.md"
        ]
      },
      "outputs": [
        "{options.outputPath}"
      ]
    }
  },
  "tags": [
    "scope:shared",
    "type:util"
  ],
  "name": "shared-payu"
}
