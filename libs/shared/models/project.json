{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/shared/models/src",
  "prefix": "smart",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/shared/models/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/shared/models"
      ],
      "options": {
        "jestConfig": "libs/shared/models/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/shared/models",
        "tsConfig": "libs/shared/models/tsconfig.lib.json",
        "packageJson": "libs/shared/models/package.json",
        "main": "libs/shared/models/src/index.ts",
        "assets": [
          "libs/shared/models/*.md"
        ]
      }
    }
  },
  "generators": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "tags": [
    "scope:shared",
    "type:util"
  ]
}
